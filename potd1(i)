SCORE OF A STRING
class Solution {
public:
    int scoreOfString(string s) {
        int score=0;
       for(int i = 0;i<s.length()-1;i++){
          score+= abs(s[i]-s[i+1]);
       } 
       return score;
    }
};

GCD OF TWO NUMBERS
class Solution {
  public:
    int gcd(int a, int b) {
      if(a%b==0)return b;
      gcd(b,a%b);
    }
};

