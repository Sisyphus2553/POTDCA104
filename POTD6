#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,m;
    cin>>n;
    cin>>m;
    int sum=0;
    int c = 0;
    for(int i = 1;i<=n;i++){
       sum+= 5*i;
 
       if(sum<=(240-m)){
         c++;
       }else{
        break;
       }
    }
    cout<<c;
}





#include<bits/stdc++.h>
using namespace std;
int main(){
  int n;
  cin>>n;
  int sum=0;
  int c = 1;
  while(1){
 
    sum+=(n/pow(5,c));
    
    if((n/pow(5,c))==0)break;
    c++;
  }
 
  cout<<sum;
